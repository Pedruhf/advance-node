{
  "name": "advanced-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "start": "node dist/src/main",
    "dev": "ts-node-dev --respawn --transpile-only --inspect=0.0.0.0:9229 src/main",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:fb-api": "npm run test -- -c ./jest.integration.config.js tests/external/facebook-api.test.ts",
    "test:aws-s3": "npm run test -- -c ./jest.integration.config.js tests/external/aws-s3-file-storage.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pedruhf/advance-node.git"
  },
